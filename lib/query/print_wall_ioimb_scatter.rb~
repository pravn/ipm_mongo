#!/usr/bin/ruby
class Print_things
    def self.print_wall_ioimb_scatter(file_name)
      file=File.new(file_name,"w")
      h=Query_operations.return_attr("banner.wallclock","gbytes",
                                     "banner.wallclock"=>{"$gt"=>@wall_filter})
      
      h.each do 
        |x|
        x.each do 
          |key,value|
          pimb=key
          wallclock=value

          puts pimb.inspect, "\t", wallclock.inspect
          file.print pimb, "\t\t", wallclock, "\n"
        end

      end

      file.close
    end
end
